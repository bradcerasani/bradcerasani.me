{
	"$schema": "https://biomejs.dev/schemas/1.6.2/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true
		}
	},
	"javascript": {
		"formatter": {
			"indentStyle": "space",
			"jsxQuoteStyle": "double",
			"lineWidth": 100,
			"quoteStyle": "single"
		}
	}
}
